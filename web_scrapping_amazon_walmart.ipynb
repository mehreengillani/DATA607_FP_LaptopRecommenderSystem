    "\n",
    "# Calculate improvement\n",
    "original_with_brand = len(amazon_df[amazon_df['brand'] != 'N/A'])\n",
    "final_with_brand = len(amazon_df[amazon_df['brand_final'] != 'N/A'])\n",
    "improvement = final_with_brand - original_with_brand\n",
